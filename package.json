{
    "name": "rssnippets",
    "displayName": "Ravenscript Snippets",
    "description": "A set of snippets to help with coding in Ravenscript.",
    "version": "1.8.0",
    "publisher": "EaglePancake",
    "repository": {
        "type": "git",
        "url": "https://github.com/EaglePancake/rssnippets2"
    },
    "icon": "ravenscriptPick2.png",
    "contributors": [
        "EaglePancake",
        "wronex"
    ],
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "lua",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "tslint -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^12.12.0",
        "tslint": "^5.19.0",
        "typescript": "^3.8.3",
        "@types/vscode": "^1.32.0"
    }
}
